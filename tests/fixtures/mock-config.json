{
  "chains": {
    "ethereum": {
      "name": "Ethereum",
      "chainId": 1,
      "rpcUrls": [
        "https://eth.llamarpc.com",
        "https://rpc.ankr.com/eth"
      ],
      "explorerApi": {
        "baseUrl": "https://api.etherscan.io/api",
        "apiKeyEnv": "ETHERSCAN_API_KEY"
      },
      "nativeToken": {
        "symbol": "ETH",
        "decimals": 18
      },
      "envVars": ["RPC_URL", "ETHEREUM_RPC_URL"]
    },
    "base": {
      "name": "Base",
      "chainId": 8453,
      "rpcUrls": [
        "https://mainnet.base.org"
      ],
      "explorerApi": {
        "baseUrl": "https://api.basescan.org/api",
        "apiKeyEnv": "BASESCAN_API_KEY"
      },
      "nativeToken": {
        "symbol": "ETH",
        "decimals": 18
      },
      "envVars": ["BASE_RPC_URL"]
    }
  },
  "defaultChain": "ethereum",
  "version": "1.0.0"
}